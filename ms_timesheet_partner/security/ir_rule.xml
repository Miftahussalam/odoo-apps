<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="hr_timesheet.timesheet_line_rule_user" model="ir.rule">
            <field name="groups" eval="[(4, ref('ms_timesheet_partner.group_hr_timesheet_partner'))]"/>
        </record>

        <record id="timesheet_line_rule_partner" model="ir.rule">
            <field name="name">account.analytic.line.timesheet.partner</field>
            <field name="model_id" ref="analytic.model_account_analytic_line"/>
            <field name="domain_force">[('project_id.partner_id', '=', user.partner_id.id)]</field>
            <field name="groups" eval="[(4, ref('ms_timesheet_partner.group_hr_timesheet_partner'))]"/>
        </record>

        <record id="timesheet_line_rule_developer" model="ir.rule">
            <field name="name">account.analytic.line.timesheet.developer</field>
            <field name="model_id" ref="analytic.model_account_analytic_line"/>
            <field name="domain_force">[('employee_id', 'in', user.employee_ids.ids)]</field>
            <field name="groups" eval="[(4, ref('ms_timesheet_partner.group_hr_timesheet_developer'))]"/>
        </record>

    </data>
</odoo>